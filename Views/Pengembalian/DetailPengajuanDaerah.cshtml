            success: function (data, textStatus, XMLHttpRequest) {
				if (data.Status) {
					swal({
						title: "Sukses",
						text: data.Pesan,
						type: "success",
						closeOnEsc: true,
						allowOutsideClick: true,
					}, function () {
						window.location.href = '@Url.Action("mon_pengembalian", "Pengembalian")';
