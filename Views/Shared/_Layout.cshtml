            color: white;
        }

        .button-primary {
            cursor: pointer;
            background-color: #337ab7;
            color: white;
        }

        .editors {
            display: none;
        }

        .back-to-top {
            cursor: pointer;
            position: fixed;
            bottom: 20px;
            right: 20px;
            display: none;
        }

        .glyphicon {
            font-size: 14px;
        }

        textarea {
            resize: none;
