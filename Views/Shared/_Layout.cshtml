            border-color: #ffffff;
            box-shadow: none !important;
            font-family: 'Varela Round', sans-serif;
            font-size: small;
        }

        .rightaligncolumn {
            text-align: right;
        }

        tr.even.selected, tr.odd.selected {
            background-color: #B0BED9 !important;
        }
        /* For Firefox */
        input[type='number'] {
            -moz-appearance: textfield;
        }
        /* Webkit browsers like Safari and Chrome */
        input[type=number]::-webkit-inner-spin-button,
        input[type=number]::-webkit-outer-spin-button {
            -webkit-appearance: none;
            margin: 0;
        }

        .btn-file {
            position: relative;
            overflow: hidden;
        }

            .btn-file input[type=file] {
                position: absolute;
                top: 0;
                right: 0;
                font-size: 100px;
                text-align: right;
                filter: alpha(opacity=0);
                opacity: 0;
                background: red;
                cursor: inherit;
                display: block;
            }

        .mytitleform {
            font-family: 'Muli', sans-serif;
            font-weight: normal;
            letter-spacing: 2px;
            word-spacing: 3px;
        }

        .mytitleform2 {
            font-family: 'Varela Round', sans-serif;
            font-weight: normal;
            letter-spacing: 2px;
            word-spacing: 3px;
            color: #169f85;
        }

        .control-label {
            text-align: right;
        }

        .navbar {
            position: unset;
        }

        .editor-active {
            cursor: pointer;
        }

        td.highlight {
            background-color: whitesmoke !important;
        }

        .glyphicon.glyphicon-search {
            font-size: 24px;
        }

        .displaytotal {
            margin: 0px;
            padding: 0px;
            text-align: right;
            font-weight: bold !important;
            border: none;
            color: #7a98b8;
        }

        .field-validation-error {
            color: #ad1010;
            font-family: 'Varela Round', sans-serif;
        }

        .input-validation-error {
            border: 1px solid #cccccc !important;
            background-color: #fff8f8 !important;
        }

        .field-validation-valid {
            display: none;
        }

        .validation-summary-errors {
            font-weight: bold !important;
            color: #ad1010 !important;
        }

        .validation-summary-valid {
            display: none;
        }

        .title_left {
            font-family: 'Proxima Nova Cn Rg', sans-serif;
        }

        .tddisabled {
            color: #de9898;
            font-weight: normal;
            font-family: 'Varela Round', sans-serif;
            /*text-decoration: line-through;*/
        }

        .tdnormal {
            font-weight: normal;
        }

