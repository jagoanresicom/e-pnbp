                                    }

                                        <tr>
                                            <td class="numeric"></td>
                                            <td class="myLabelContentRight">Total</td>
                                            <td class="myLabelContentRight">@Model.Sum(a => a.PaguOps).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
                                            <td class="myLabelContentRight">@Model.Sum(a => a.AlokasiOps).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
                                            <td class="myLabelContentRight">@Model.Sum(a => a.PaguNonOps).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
                                            <td class="myLabelContentRight">@Model.Sum(a => a.AlokasiNonOps).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
