                                            <td class="myLabelContentRight">@Model.Sum(a => a.AlokJul).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
                                            <td class="myLabelContentRight">@Model.Sum(a => a.AnggAgt).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
                                            <td class="myLabelContentRight">@Model.Sum(a => a.AlokAgt).ToString("N0", new System.Globalization.CultureInfo("id-ID"))</td>
