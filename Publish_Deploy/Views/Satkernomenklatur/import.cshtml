                                    <input type="file" name="import" id="import" />
                                </div>

                                <button class="btn btn-primary">Simpan</button>
                                <a href="@Url.Action("Index", "Satkernomenklatur")" class="btn btn-danger"><i class="fa fa-rotate-left"></i>&nbsp;Kembali</a>
